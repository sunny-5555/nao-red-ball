        #self.logger.info(str(time.time()*1000) + str(position_red_ball_reached))

        ballLost = 0
        atBall = False
        self.tracker.track(self.targetName)

        while(atBall==False):
            if(self.isRunning):
                self.tracker.stopTracker()
                self.end()
                self.targetReached()

